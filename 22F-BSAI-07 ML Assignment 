   "source": [
    "# Initialize Model\n",
    "ensemble_model = RandomForestClassifier(n_estimators=100, random_state=42)\n",
    "\n",
    "ensemble_model.fit(X_train, y_train)\n",
    "y_pred_ens = ensemble_model.predict(X_test)\n",
    "\n",
    "# Evaluation of Model\n",
    "results[\"Ensemble (Random Forest)\"] = {\n",
    "    \"Accuracy\": accuracy_score(y_test, y_pred_ens),\n",
    "    \"Precision\": precision_score(y_test, y_pred_ens),\n",
    "    \"Recall\": recall_score(y_test, y_pred_ens),\n",
    "    \"F1 Score\": f1_score(y_test, y_pred_ens),\n",
    "    \"Predictions\": y_pred_ens\n",
    "}\n",
    "\n",
    "print(\"Model has been trained successfully.\")"
   ]
